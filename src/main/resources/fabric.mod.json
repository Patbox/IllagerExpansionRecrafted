{
  "schemaVersion": 1,
  "id": "illagerexp",
  "version": "1.1.0",

  "name": "Illager Expansion [Rematch]",
  "description": "A Fabric Mod that focuses on expanding content surrounding the mysterious Illagers in Minecraft!",
  "authors": [
    "Dricky",
    "Safro",
    "Tabzlock",
    "All other contributors :)"
  ],
  "contact": {
    "sources": "https://github.com/ZekeSmith/Illager-Expansion-Rematch",
    "issues": "https://github.com/ZekeSmith/Illager-Expansion-Rematch/issues",
    "homepage": "https://pb4.eu"
  },

  "license": "MIT",
  "icon": "assets/illagerexp/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.sandbox.IllagerExpansion"
    ],
    "mm:early_risers": [
      "me.sandbox.EarlyRiser"
    ]
  },
  "mixins": [
    "illagerexp.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.3",
    "fabric": "*",
    "minecraft": "1.20.4",
    "java": ">=17",
    "mm": ">=2.3"
  },
  "accessWidener" : "illagerexp.accesswidener"
